{
    "name": "aw-studio/macrame-blocks",
    "license": "MIT",
    "authors": [
        "jannes@aw-studio.de"
    ],
    "autoload": {
        "psr-4": {
            "AwStudio\\Blocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "php": "^8.1",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0"
    },
    "require": {
        "illuminate/support": "^9.0",
        "illuminate/contracts": "^9.0",
        "inertiajs/inertia-laravel": "^0.5",
        "laravel/breeze": "^1.8",
        "macramejs/macrame-laravel": "dev-main"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AwStudio\\Blocks\\BlocksServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
